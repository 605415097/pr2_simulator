<launch>
  <include file="$(find pr2_alpha)/sim.machine"/>
  <!-- start up empty world -->
  <include file="$(find gazebo)/launch/empty_world.launch"/>

  <param name="robot_description" command="$(find xacro)/xacro.py '$(find texas)/texas.urdf.xacro'" />
  <node name="spawn_texas" pkg="gazebo_tools" type="spawn_gazebo_model"  args="robot_description" respawn="false" output="screen"/>

  <!-- Controller Manager -->
  <include file="$(find pr2_controller_manager)/controller_manager.launch" />

</launch>

