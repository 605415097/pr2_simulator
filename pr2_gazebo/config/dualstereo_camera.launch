<launch>
  <!-- Stereo image processing -->
  <node ns="wide_stereo" name="wide_stereo_proc" pkg="stereo_image_proc" type="stereo_image_proc" respawn="false" output="screen">
    <param name="disparity_range" type="int" value="128"/>
  </node>
  <node ns="narrow_stereo" name="narrow_stereo_proc" pkg="stereo_image_proc" type="stereo_image_proc" respawn="false" output="screen">
    <param name="disparity_range" type="int" value="128"/>
  </node>

  <node name="copy_narrow_stereo_textured_left_image"  pkg="topic_tools" type="mux" args="/narrow_stereo_textured/left/image_raw    /narrow_stereo/left/image_raw"/>
  <node name="copy_narrow_stereo_textured_left_info"   pkg="topic_tools" type="mux" args="/narrow_stereo_textured/left/camera_info  /narrow_stereo/left/camera_info"/>
  <node name="copy_narrow_stereo_textured_right_image" pkg="topic_tools" type="mux" args="/narrow_stereo_textured/right/image_raw   /narrow_stereo/right/image_raw"/>
  <node name="copy_narrow_stereo_textured_right_info"  pkg="topic_tools" type="mux" args="/narrow_stereo_textured/right/camera_info /narrow_stereo/right/camera_info"/>
  <node ns="narrow_stereo_textured" name="narrow_stereo_textured_proc" pkg="stereo_image_proc" type="stereo_image_proc" respawn="false" output="screen">
    <param name="disparity_range" type="int" value="128"/>
  </node>
</launch>


